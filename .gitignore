# Combined .gitignore for Frontend and Backend

# Ignore node_modules for both frontend and backend
/frontend/node_modules/
/backend/node_modules/

# Ignore build and other build-related directories for the frontend
/frontend/build/

# Ignore logs, pids, and other runtime-related directories for the backend
/backend/logs/
/backend/pids/
/backend/pm2/

# Ignore environment files for both
.env.local
.env.development
.env.test
.env.production

# Ignore editor and system files
.idea/
.vscode/
.DS_Store
.editorconfig
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw*
.npm
